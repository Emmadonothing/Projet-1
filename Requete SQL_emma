--------Stock Flop 5 =


SELECT 
    productCode,
    productName,
    quantityInStock
FROM 
    products
ORDER BY 
    quantityInStock ASC
LIMIT 5;


-------Stock Top 5 = 

SELECT
  productCode,
  productName,
  quantityInStock
FROM
  products
ORDER BY
  quantityInStock DESC
LIMIT
  5;

-- Correction Imad stock top 5 =

SELECT
  products.productCode,
  productName,
  quantityInStock,
  SUM(quantityOrdered) AS Qte_commande
FROM
  products
JOIN 
orderdetails ON orderdetails.productCode = products.productCode
GROUP BY products.productCode
ORDER BY
  Qte_commande DESC
LIMIT
  5;





